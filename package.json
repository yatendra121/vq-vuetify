{
    "name": "@qnx/vuetify",
    "version": "0.6.12",
    "type": "module",
    "license": "MIT",
    "author": "Yatendra Kushwaha <yatendra121kushwaha@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/yatendra121/vq-vuetify.git"
    },
    "scripts": {
        "serve": "vite preview",
        "build": "vite build && vue-tsc --pretty  --declaration --emitDeclarationOnly",
        "dev": "vite",
        "build:types": "rollup --config build/rollup.types.config.js",
        "build-force": "pnpm build && pnpm build:types",
        "test:unit": "vitest",
        "format": "prettier .  --write",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/yatendra121/vq-vuetify/issues"
    },
    "keywords": [
        "vue",
        "components",
        "form",
        "form components",
        "vuetify components"
    ],
    "engines": {
        "node": "^12.20 || >=14.13"
    },
    "sideEffects": false,
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./*": "./*",
        "./integrations": {
            "import": "./src/integrations.ts",
            "types": "./src/integrations.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/index.ts"
            ],
            "integrations": [
                "src/integrations.ts"
            ]
        }
    },
    "publishConfig": {
        "main": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "require": "./dist/index.js"
            },
            "./*": "./*",
            "./integrations": {
                "types": "./dist/integrations.d.ts",
                "require": "./dist/integrations.js",
                "import": "./dist/integrations.js"
            }
        },
        "typesVersions": {
            "*": {
                "*": [
                    "dist/index.d.ts"
                ],
                "integrations": [
                    "dist/integrations.d.ts"
                ]
            }
        }
    },
    "dependencies": {
        "@qnx/composables": "*",
        "vue": "^3.4.19"
    },
    "devDependencies": {
        "@mdi/js": "^7.4.47",
        "@rollup/plugin-alias": "^5.1.0",
        "@rushstack/eslint-patch": "^1.7.2",
        "@tinymce/tinymce-vue": "^5.1.1",
        "@types/node": "^20.11.16",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "2.4.4",
        "axios": "^1.6.7",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.21.1",
        "fast-glob": "^3.3.2",
        "jsdom": "^24.0.0",
        "micromatch": "^4.0.5",
        "path": "^0.12.7",
        "pinia": "^2.1.7",
        "prettier": "^3.2.5",
        "roboto-fontface": "^0.10.0",
        "rollup": "^4.9.6",
        "rollup-plugin-dts": "^6.1.0",
        "sass": "^1.70.0",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "vee-validate": "^4.12.5",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vite-plugin-vuetify": "^2.0.1",
        "vitest": "^1.2.2",
        "vue-router": "^4.2.5",
        "vue-tsc": "^1.8.27",
        "vuetify": "^3.5.14",
        "webfontloader": "^1.6.28",
        "yup": "^1.3.3"
    },
    "peerDependencies": {
        "@qnx/composables": "*",
        "axios": "*",
        "pinia": "*",
        "vee-validate": "*",
        "vue": "*",
        "vuetify": "*",
        "yup": "*"
    },
    "peerDependenciesMeta": {
        "@tinymce/tinymce-vue": {
            "optional": true
        }
    }
}
