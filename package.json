{
  "name": "vq-vuetify",
  "version": "0.5.41",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/yatendra121/vq-vuetify.git"
  },
  "scripts": {
    "serve": "vite preview",
    "build": "vite build && vue-tsc --pretty  --declaration --emitDeclarationOnly",
    "dev": "vite",
    "build:types": "rollup --config build/rollup.types.config.js",
    "build-force": "pnpm build && pnpm build:types"
  },
  "files": [
    "dist"
  ],
  "main": "./src/main.ts",
  "module": "./src/main.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vq-vuetify.es.js",
      "require": "./dist/vq-vuetify.es.js"
    }
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@mdi/js": "^7.1.96",
    "@rollup/plugin-alias": "^4.0.2",
    "@types/node": "^18.11.17",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "axios": "^1.2.1",
    "fast-glob": "^3.2.12",
    "micromatch": "^4.0.5",
    "path": "^0.12.7",
    "pinia": "^2.0.28",
    "roboto-fontface": "*",
    "rollup": "^3.8.1",
    "rollup-plugin-dts": "^5.1.0",
    "sass": "^1.57.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vee-validate": "^4.7.3",
    "vite": "4.0.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-vuetify": "*",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.16",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.6.28",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "axios": "*",
    "vee-validate": "^4.5.6",
    "vue": "^3.2.0",
    "vuetify": "*",
    "yup": "^0.32.11"
  }
}
