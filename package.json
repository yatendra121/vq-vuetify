{
    "name": "@qnx/vuetify",
    "version": "0.5.78",
    "type": "module",
    "license": "MIT",
    "author": "Yatendra Kushwaha <yatendra121kushwaha@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/yatendra121/vq-vuetify.git"
    },
    "scripts": {
        "serve": "vite preview",
        "build": "vite build && vue-tsc --pretty  --declaration --emitDeclarationOnly",
        "dev": "vite",
        "build:types": "rollup --config build/rollup.types.config.js",
        "build-force": "pnpm build && pnpm build:types",
        "test:unit": "vitest",
        "format": "prettier .  --write",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/yatendra121/vq-vuetify/issues"
    },
    "keywords": [
        "vue",
        "components",
        "form",
        "form components"
    ],
    "engines": {
        "node": "^12.20 || >=14.13"
    },
    "sideEffects": false,
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./*": "./*",
        "./integrations": {
            "import": "./src/integrations.ts",
            "types": "./src/integrations.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "src/index.ts"
            ],
            "integrations": [
                "src/integrations.ts"
            ]
        }
    },
    "publishConfig": {
        "main": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "require": "./dist/index.js"
            },
            "./*": "./*",
            "./integrations": {
                "types": "./dist/integrations.d.ts",
                "require": "./dist/integrations.js",
                "import": "./dist/integrations.js"
            }
        },
        "typesVersions": {
            "*": {
                "*": [
                    "dist/index.d.ts"
                ],
                "integrations": [
                    "dist/integrations.d.ts"
                ]
            }
        }
    },
    "dependencies": {
        "@qnx/composables": "^0.0.14",
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@mdi/js": "^7.1.96",
        "@rollup/plugin-alias": "^4.0.3",
        "@rushstack/eslint-patch": "^1.1.4",
        "@tinymce/tinymce-vue": "^5.0.0",
        "@types/node": "^18.14.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "2.2.10",
        "axios": "^1.3.3",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "fast-glob": "^3.2.12",
        "jsdom": "^21.1.0",
        "micromatch": "^4.0.5",
        "path": "^0.12.7",
        "pinia": "^2.0.30",
        "prettier": "^2.7.1",
        "roboto-fontface": "^0.10.0",
        "rollup": "^3.17.2",
        "rollup-plugin-dts": "^5.2.0",
        "sass": "^1.58.3",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vee-validate": "^4.7.4",
        "vite": "4.0.1",
        "vite-plugin-dts": "^1.7.3",
        "vite-plugin-vuetify": "^1.0.2",
        "vitest": "^0.25.8",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.1.4",
        "vuetify": "3.1.4",
        "webfontloader": "^1.6.28",
        "yup": "^0.32.11"
    },
    "peerDependencies": {
        "@qnx/composables": "*",
        "axios": "*",
        "pinia": "*",
        "vee-validate": "*",
        "vue": "*",
        "vuetify": "*",
        "yup": "*"
    },
    "peerDependenciesMeta": {
        "@tinymce/tinymce-vue": {
            "optional": true
        }
    }
}
